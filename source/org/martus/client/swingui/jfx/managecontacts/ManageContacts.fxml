<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="345.0" prefWidth="810.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <BorderPane prefHeight="345.0" prefWidth="682.0">
      <bottom>
        <VBox prefHeight="230.0" prefWidth="796.0" BorderPane.alignment="CENTER">
          <children>
            <TableView id="contacts-table" fx:id="contactsTable" editable="true" prefHeight="230.0" prefWidth="794.0">
              <columns>
                <TableColumn id="" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="106.0" sortable="true" text="Contact Name" fx:id="contactNameColumn" />
                <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="145.0" text="Send To by Default" fx:id="sendToByDefaultColumn" />
                <TableColumn editable="false" id="" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="394.0" sortable="true" text="Public Code" fx:id="publicCodeColumn" />
                <TableColumn editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="81.0" resizable="false" sortable="true" text="Verified" fx:id="verificationStatusColumn" />
                <TableColumn editable="false" id="" maxWidth="5000.0" minWidth="10.0" prefWidth="62.0" resizable="false" sortable="false" text="Remove" fx:id="removeContactColumn" />
              </columns>
            </TableView>
          </children>
        </VBox>
      </bottom>
      <top>
        <VBox prefHeight="93.0" prefWidth="672.0">
          <children>
            <Label text="Manage Contacts" />
            <Label prefHeight="56.0" prefWidth="794.0" text="To send bulletins to your contacts, you will need their public code. You will also want to share your public code with your contacts so they can verify your key." wrapText="true" />
            <Label text="" />
            <HBox prefHeight="30.0" prefWidth="726.0">
              <children>
                <Label prefWidth="116.0" text="Access Token:" />
                <TextField id="" fx:id="accessTokenField" promptText="Access Token" />
                <Button id="" fx:id="addContactButtonFromToken" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#addContactFromToken" prefWidth="264.0" text="Add Contact Using Access Token..." />
                <Separator prefWidth="94.0" />
                <Button fx:id="addContactButtonFromFile" mnemonicParsing="false" onAction="#addContactFromFile" prefWidth="205.0" text="Add Contact from File..." />
              </children>
            </HBox>
          </children>
        </VBox>
      </top>
    </BorderPane>
  </children>
</VBox>
