<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox>
	<Label styleClass="headingLevel3" text="Import from my contacts" />
	<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	  <columnConstraints>
	    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
	    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
	  </columnConstraints>
	  <rowConstraints>
	    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
	<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" />
	    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
	    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
	  </rowConstraints>
		<children>
			<ChoiceBox fx:id="contactsChoiceBox" prefWidth="150.0" GridPane.rowIndex="2" xmlns:fx="http://javafx.com/fxml" />
			<ChoiceBox fx:id="templatesChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" xmlns:fx="http://javafx.com/fxml" />
			<Label styleClass="headingLevel3"  fx:id="continueLabel" text="Import from this contact" GridPane.rowIndex="1" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" />
			<Label styleClass="headingLevel3" fx:id="chooseFormTemplateLabel" text="Choose a Form" GridPane.columnIndex="1" GridPane.rowIndex="1" />
			<Label styleClass="headingLevel3"  fx:id="noTemplatesAvailableLabel" text="No Templates Available" GridPane.columnIndex="1" GridPane.rowIndex="2" />
			<Button fx:id="continueButton" mnemonicParsing="false" onAction="#onContinue" text="Continue" GridPane.columnIndex="1" GridPane.rowIndex="2" />
		<Label styleClass="headingLevel3" text="Choose a Contact" GridPane.rowIndex="1" />
		<Button mnemonicParsing="false" onAction="#onCancel" text="Cancel" GridPane.columnIndex="3" />
		</children>
	</GridPane>
</VBox>