<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="345.0" prefWidth="640.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <BorderPane prefHeight="346.0" prefWidth="668.0">
      <bottom>
        <VBox prefHeight="230.0" prefWidth="898.0" BorderPane.alignment="CENTER">
          <children>
            <TableView id="" fx:id="contactsTable" editable="true" prefHeight="238.0" prefWidth="814.0">
              <columns>
                <TableColumn id="" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="120.0" text="Contact Name" fx:id="contactNameColumn" />
                <TableColumn id="" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="204.0" text="Public Code" fx:id="publicCodeColumn" />
                <TableColumn id="" maxWidth="5000.0" minWidth="10.0" prefWidth="99.0" text="Send" fx:id="canSendToColumn" />
                <TableColumn id="" maxWidth="5000.0" minWidth="10.0" prefWidth="83.0" text="Receive" fx:id="canReceiveFromColumn" />
                <TableColumn id="" maxWidth="5000.0" minWidth="10.0" prefWidth="131.0" text="Remove Contact" fx:id="removeContactColumn" />
              </columns>
            </TableView>
          </children>
        </VBox>
      </bottom>
      <top>
        <VBox prefHeight="93.0">
          <children>
            <Label text="Add Contacts" />
            <Label prefHeight="91.0" prefWidth="640.0" text="To send bulletins to your contacts, you will need their public code. You willalso want to share your public code with your contacts so they can verify your key. You can add contacts from the Martus desktop if you don't know them now." wrapText="true" />
            <HBox prefHeight="53.0" prefWidth="839.0">
              <children>
                <Label text="Access Token:" />
                <TextField id="" fx:id="accessTokenField" promptText="Access Token" />
                <Button id="" fx:id="addContactButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#addContact" prefWidth="155.0" text="Add Contact..." />
              </children>
            </HBox>
          </children>
        </VBox>
      </top>
    </BorderPane>
  </children>
</VBox>
