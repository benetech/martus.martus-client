<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="375.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <BorderPane prefHeight="345.0">
      <left>
        <VBox prefHeight="230.0" prefWidth="898.0" BorderPane.alignment="CENTER">
          <children>
            <HBox prefHeight="48.0" prefWidth="996.0">
              <children>
                <Label text="Access Token:" />
                <TextField fx:id="accessTokenField" promptText="Access Token" />
                <Button fx:id="addContactButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#addContact" prefWidth="155.0" text="Add Contact..." />
              </children>
            </HBox>
            <TableView fx:id="contactsTable" editable="true" prefHeight="276.0" prefWidth="982.0">
              <columns>
                <TableColumn maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="120.0" text="Contact Name" fx:id="contactNameColumn" />
                <TableColumn maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="297.0" text="Public Code" fx:id="publicCodeColumn" />
                <TableColumn id="canSendToColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="99.0" text="Send" fx:id="canSendToColumn" />
                <TableColumn id="canReceiveFromColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="83.0" text="Receive" fx:id="canReceiveFromColumn" />
              </columns>
            </TableView>
          </children>
        </VBox>
      </left>
      <top>
        <VBox prefHeight="93.0">
          <children>
            <Label text="Add Contacts" />
            <Label prefHeight="59.0" prefWidth="898.0" text="To send bulletins to your contacts, you will need their public code. You willalso want to share your public code with your contacts so they can verify your key. You can add contacts from the Martus desktop if you don't know them now." wrapText="true" />
          </children>
        </VBox>
      </top>
    </BorderPane>
  </children>
</VBox>
