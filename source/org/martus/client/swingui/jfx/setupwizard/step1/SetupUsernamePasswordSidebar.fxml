<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="427.9998779296875" prefWidth="300.0" maxWidth="300" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <VBox styleClass="sidebar" fx:id="fxVBoxUserNameTips" prefHeight="215.0" prefWidth="200.0" maxHeight="215" maxWidth="200">
      <children>
        <Label styleClass="sidebarTextHeading1" text="UserName Tips:" wrapText="true" />
        <Label styleClass="sidebarText" text="- Your username can contain letters, numbers, punctuation and spaces." wrapText="true" />
        <Label styleClass="sidebarText" text="- It should have between 8-50 characters, and it is case sensitive." wrapText="true" />
        <Label styleClass="sidebarText" text="- Remember to choose a username you can remember." wrapText="true" />
      </children>
    </VBox>


    <VBox styleClass="sidebar" fx:id="fxVBoxPasswordTips" prefHeight="215.0" prefWidth="200.0" maxHeight="215" maxWidth="200.0">
      <children>
        <Label styleClass="sidebarTextHeading1" text="Password Tips:" wrapText="true" />
        <Label styleClass="sidebarText" text="- Do not use your username" wrapText="true" />
        <Label styleClass="sidebarText" text="- Do not use a single dictionary word" wrapText="true" />
        <Label styleClass="sidebarText" text="- Use at least 15 characters for most security (8 are required)" wrapText="true" />
        <Label styleClass="sidebarText" text="- Use a combination of alphabet characters, numbers, and special characters (such as !@#$%^*&amp;)" wrapText="true" />
        <Label styleClass="sidebarText" text="- Use a combination of uppercase (capital) and lowercase characters if you use a case-sensitive language." wrapText="true" />
      </children>
    </VBox>
  </children>
</StackPane>
